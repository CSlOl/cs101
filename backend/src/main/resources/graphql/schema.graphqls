type Problem {
    id : ID!
    title : String!
    question : String!
    option1 : String!
    option2 : String!
    option3 : String!
    option4 : String!
    answer : String!
    description : String!
    isWithdrawn : String!
    type: String!
    category: String!
}

type Query {
    getProblem(id: ID!): Problem
    getProblems : [Problem]
}